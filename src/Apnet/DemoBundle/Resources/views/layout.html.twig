{% extends "ApnetLayoutBundle::body.html.twig" %}

{% block stylesheets_core %}
    <link rel="stylesheet" href="{{ asset('components/bootstrap/css/bootstrap.min.css') }}"/>
    {% stylesheets filter="cssrewrite"
      "bundles/apnetdemo/sass/style.scss"
      "bundles/apnetdemo/sass/block-social.scss"
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    <!--[if lt IE 9]><script type="text/javascript" src="{{ asset('components/html5shiv/dist/html5shiv.js') }}"></script><![endif]-->
{% endblock %}

{% block head %}
    {{ parent() }}
    <link rel="shortcut icon" href="{{ asset('bundles/apnetdemo/ico/favicon.ico') }}">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ asset('bundles/apnetdemo/ico/apple-touch-icon-144-precomposed.png') }}">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{ asset('bundles/apnetdemo/ico/apple-touch-icon-114-precomposed.png') }}">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="{{ asset('bundles/apnetdemo/ico/apple-touch-icon-72-precomposed.png') }}">
    <link rel="apple-touch-icon-precomposed" href="{{ asset('bundles/apnetdemo/ico/apple-touch-icon-57-precomposed.png') }}">
{% endblock %}
